                $("#runBtn").text("2");
                }, 1000);
              setTimeout(function() {
                $("#runBtn").text("1");
                }, 2000);
              setTimeout(function() {
                $("#loader").remove();
                $("#runBtn").remove();
                }, 3000);
            });

              }, 2000);*/


    });
    
