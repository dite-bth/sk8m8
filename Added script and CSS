            $(".btn").mouseout(function(){
              $(this).css("opacity", "0.6");
            });

            $('#backBtn').click(function() {
            location.reload();
            //$(this).remove();
            });
            $('#runBtn').click(function() {
              $("#box").append("<div id='loader'></div>");
              $("#runBtn").text("3");

