//Joakim
 //Funktion för att expandera "session"-diven.
  $('#box').click(function() {

          $("#photonDiv").slideToggle("slow");
          if (sessionExpand) {
            $("#arrow").hide();
            $("#backButton").hide();
            $("#startPhoton").hide();
            $("#info").hide();
            $(this).css('font-size', '350%');
            $(this).text('Begin Session');
            setTimeout(function() {
            $("#history").show();
            $("#about").show();
            $(this).css('opacity', '0.6');
            }, 600);
            sessionExpand = false;
            expanded = false;
          }
          else{
            $(this).text('Start Session');
            $(this).css('opacity', '1.0');
            $(this).text('^');
            $(this).css('font-size', '500%');
            $("#history").hide();
            $("#about").hide();
            setTimeout(function() {
              $("#arrow").show();
              $("#backButton").show();
              $("#startPhoton").show();
              $("#info").show();
            }, 600);
            sessionExpand = true;
            expanded = true;
          }



          /*setTimeout(function() {
